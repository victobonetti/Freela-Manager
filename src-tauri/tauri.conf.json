{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "FiadoPro",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.fiadopro.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
      "updater": {
      "active": true,
      "endpoints": [
        "http://localhost:3000/api/{{target}}/{{arch}}/{{current_version}}"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDNFQ0Y4MjZENEI4NDIwNTEKUldSUklJUkxiWUxQUHBhbDR2a1ozZ0hMajFCOFlFZFhka1g5Q3VIMVFVblpreks3bkJZMUs5UEsK"
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "FiadoPro",
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ]
  }
}
